    <script type="text/javascript">
		$(document).ready(function () {

			if ('@TempData["message"]' == "Added") {
				$(".alert-success").show();
				window.setTimeout(function() {
          $(".alert-success").slideUp(1000, function(){
        $(this).remove();
          });
         }, 5000);
       }
			if ('@TempData["message"]' == 'false') {
				$(".alert-danger").show();
				window.setTimeout(function() {
          $(".alert-danger").slideUp(1000, function(){
        $(this).remove();
          });
         }, 5000);
			}
    });
    </script>
